{
  "name": "image-opt",
  "version": "0.1.0",
  "description": "Optimize your source images for the web",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s build:*",
    "build:cli": "node scripts/build.mjs",
    "build:frontend": "cd ./packages/frontend && pnpm build && cd -"
  },
  "keywords": [
    "image-optimization"
  ],
  "bin": {
    "typed": "bin/io"
  },
  "type": "module",
  "module": "./packages/frontend/dist/ImgOpt.mjs",
  "exports": {
    ".": {
      "import": "./packages/frontend/dist/ImgOpt.mjs"
    },
    "./vue": {
      "import": "./packages/frontend/dist/ImgOpt.mjs"
    },
    "./custom": {
      "import": "./packages/frontend/dist/ImgElement.mjs"
    }
  },
  "author": "Ken Snyder <ken@ken.net>",
  "license": "MIT",
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-usage": "^5.0.4",
    "@types/node": "^22.5.4",
    "@vitest/ui": "^2.0.5",
    "bumpp": "^9.5.2",
    "npm-run-all": "^4.1.5",
    "pathe": "^1.1.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "typecheck": "^0.1.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@yankeeinlondon/ask": "^0.2.9",
    "chalk": "^5.3.0",
    "command-line-args": "^6.0.0",
    "command-line-usage": "^7.0.3",
    "fast-glob": "^3.3.2",
    "inferred-types": "^0.49.8",
    "inquirer": "^10.2.2",
    "sharp": "^0.33.5",
    "xxhash-wasm": "^1.0.2"
  }
}
