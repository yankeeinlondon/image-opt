  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.5.2",
    "pnpm": "^9.10.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.4",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-vue-devtools": "^7.4.5",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@vueuse/core": "^11.0.3",
    "inferred-types": "^0.49.9",
