{
  "name": "frontend",
  "version": "0.2.0",
  "description": "A frontend for displaying optimized images from image-opt CLI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "echo \"no tests yet\" && exit 0"
  },
  "module": "./dist/ImageOpt.mjs",
  "types": "./dist/ImageOpt.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ImageOpt.mjs",
      "types": "./dist/ImageOpt.d.ts"
    },
